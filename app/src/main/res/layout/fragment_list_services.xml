<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:background="@color/White"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <include
        android:id="@+id/app_bar"
        layout="@layout/appbar_all_activity"/>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textColor="@color/black"
                android:textSize="@dimen/txt_16sp"
                android:background="@color/LightGrey"
                android:text="Thuê xe"
                android:padding="@dimen/dp7"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/app_bar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:padding="@dimen/dp5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/txt_thuexemay"
                    android:textColor="@color/black"
                    android:textSize="@dimen/txt_16sp"
                    android:text="Thuê xe máy"
                    android:padding="@dimen/dp10"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <ImageView
                    android:layout_marginRight="@dimen/dp5"
                    android:tint="@color/Silver"
                    app:layout_constraintHeight_percent="0.4"
                    android:src="@drawable/ic_right_setup"
                    app:layout_constraintDimensionRatio="1:1"
                    app:layout_constraintTop_toTopOf="@+id/txt_thuexemay"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_thuexemay"
                    app:layout_constraintEnd_toEndOf="@+id/txt_thuexemay"
                    android:layout_width="0dp"
                    android:layout_height="0dp"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <View
                android:layout_marginLeft="@dimen/dp7"
                android:layout_marginRight="@dimen/dp7"
                android:background="@color/Silver"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp0_7"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:padding="@dimen/dp5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/txt_thue_oto"
                    android:textColor="@color/black"
                    android:textSize="@dimen/txt_16sp"
                    android:text="Thuê ô tô"
                    android:padding="@dimen/dp10"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <ImageView
                    android:layout_marginRight="@dimen/dp5"
                    android:tint="@color/Silver"
                    app:layout_constraintHeight_percent="0.4"
                    android:src="@drawable/ic_right_setup"
                    app:layout_constraintDimensionRatio="1:1"
                    app:layout_constraintTop_toTopOf="@+id/txt_thue_oto"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_thue_oto"
                    app:layout_constraintEnd_toEndOf="@+id/txt_thue_oto"
                    android:layout_width="0dp"
                    android:layout_height="0dp"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <TextView
                android:textColor="@color/black"
                android:textSize="@dimen/txt_16sp"
                android:background="@color/LightGrey"
                android:text="Duy thuyền"
                android:padding="@dimen/dp7"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/app_bar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:padding="@dimen/dp5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/txt_duthuyen_rieng"
                    android:textColor="@color/black"
                    android:textSize="@dimen/txt_16sp"
                    android:text="Thuê ô tô"
                    android:padding="@dimen/dp10"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <ImageView
                    android:layout_marginRight="@dimen/dp5"
                    android:tint="@color/Silver"
                    app:layout_constraintHeight_percent="0.4"
                    android:src="@drawable/ic_right_setup"
                    app:layout_constraintDimensionRatio="1:1"
                    app:layout_constraintTop_toTopOf="@+id/txt_duthuyen_rieng"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_duthuyen_rieng"
                    app:layout_constraintEnd_toEndOf="@+id/txt_duthuyen_rieng"
                    android:layout_width="0dp"
                    android:layout_height="0dp"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <View
                android:layout_marginLeft="@dimen/dp7"
                android:layout_marginRight="@dimen/dp7"
                android:background="@color/Silver"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp0_7"/>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:padding="@dimen/dp5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/txt_thuyenthamvinh"
                    android:textColor="@color/black"
                    android:textSize="@dimen/txt_16sp"
                    android:text="Thuê ô tô"
                    android:padding="@dimen/dp10"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <ImageView
                    android:layout_marginRight="@dimen/dp5"
                    android:tint="@color/Silver"
                    app:layout_constraintHeight_percent="0.4"
                    android:src="@drawable/ic_right_setup"
                    app:layout_constraintDimensionRatio="1:1"
                    app:layout_constraintTop_toTopOf="@+id/txt_thuyenthamvinh"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_thuyenthamvinh"
                    app:layout_constraintEnd_toEndOf="@+id/txt_thuyenthamvinh"
                    android:layout_width="0dp"
                    android:layout_height="0dp"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <TextView
                android:textColor="@color/black"
                android:textSize="@dimen/txt_16sp"
                android:background="@color/LightGrey"
                android:text="Dịch vụ khác"
                android:padding="@dimen/dp7"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/app_bar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:padding="@dimen/dp5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/txt_thuyphico"
                    android:textColor="@color/black"
                    android:textSize="@dimen/txt_16sp"
                    android:text="Thuê ô tô"
                    android:padding="@dimen/dp10"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <ImageView
                    android:layout_marginRight="@dimen/dp5"
                    android:tint="@color/Silver"
                    app:layout_constraintHeight_percent="0.4"
                    android:src="@drawable/ic_right_setup"
                    app:layout_constraintDimensionRatio="1:1"
                    app:layout_constraintTop_toTopOf="@+id/txt_thuyphico"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_thuyphico"
                    app:layout_constraintEnd_toEndOf="@+id/txt_thuyphico"
                    android:layout_width="0dp"
                    android:layout_height="0dp"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <View
                android:layout_marginLeft="@dimen/dp7"
                android:layout_marginRight="@dimen/dp7"
                android:background="@color/Silver"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp0_7"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:padding="@dimen/dp5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/txt_tructhang"
                    android:textColor="@color/black"
                    android:textSize="@dimen/txt_16sp"
                    android:text="Thuê ô tô"
                    android:padding="@dimen/dp10"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <ImageView
                    android:layout_marginRight="@dimen/dp5"
                    android:tint="@color/Silver"
                    app:layout_constraintHeight_percent="0.4"
                    android:src="@drawable/ic_right_setup"
                    app:layout_constraintDimensionRatio="1:1"
                    app:layout_constraintTop_toTopOf="@+id/txt_tructhang"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_tructhang"
                    app:layout_constraintEnd_toEndOf="@+id/txt_tructhang"
                    android:layout_width="0dp"
                    android:layout_height="0dp"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>




</LinearLayout>